<?xml version="1.0" encoding="utf-8"?>
<!-- xmlnsを消してはいけない -->
<appengine-web-app xmlns="http://appengine.google.com/ns/1.0">
    <threadsafe>true</threadsafe>
    <runtime>java8</runtime>
    <module>your_module_name</module>
    <env-variables>
        <env-var name="TWITTER_API_KEY" value="your_key" />
        <env-var name="TWITTER_API_SECRET_KEY" value="your_secret_key" />
        <env-var name="TWITTER_REDIRECT_URL" value="your_redirect_url" />
    </env-variables>
</appengine-web-app>
